
strict-order
domain-needed
domain=woitasen.com.ar
expand-hosts
local=/woitasen.com.ar/
no-resolv
server=8.8.8.8
server=8.8.4.4
pid-file=/var/run/dnsmasq.pid
except-interface=lo
bind-dynamic
interface=eth1
addn-hosts=/var/lib/dnsmasq/default.addnhosts
dhcp-option=3,<%= @subnet %>.1
dhcp-range=<%= @subnet %>.100,<%= @subnet %>.200
dhcp-no-override
dhcp-leasefile=/var/lib/dnsmasq/default.leases
dhcp-lease-max=253
dhcp-hostsfile=/var/lib/dnsmasq/default.hostsfile

